<h1>Agentes</h1>

<table>
  <thead>
  <tr>
    <th>Nombre</th>
    <th>Email</th>
    <th>Company</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @agents.each do |a| %>
      <tr>
        <td><%= a.name %></td>
        <td><%= a.email %></td>
        <td><%= a.company %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>
